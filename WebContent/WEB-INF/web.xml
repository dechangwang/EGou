<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
 <display-name>EGou</display-name>
 <servlet>
  <display-name>CustomerLogOnServlet</display-name>
  <servlet-name>CustomerLogOnServlet</servlet-name>
  <servlet-class>edu.tongji.egou.servlet.CustomerLogOnServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>CustomerRegisterServlet</display-name>
  <servlet-name>CustomerRegisterServlet</servlet-name>
  <servlet-class>edu.tongji.egou.servlet.CustomerRegisterServlet</servlet-class>
 </servlet>
 <servlet>
  <display-name>InitServlet</display-name>
  <servlet-name>InitServlet</servlet-name>
  <servlet-class>edu.tongji.egou.servlet.InitServlet</servlet-class>
  <load-on-startup>2</load-on-startup>
 </servlet>
 <servlet-mapping>
  <servlet-name>CustomerLogOnServlet</servlet-name>
  <url-pattern>/CustomerLogOnServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>CustomerRegisterServlet</servlet-name>
  <url-pattern>/CustomerRegisterServlet</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>InitServlet</servlet-name>
  <url-pattern>/InitServlet</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>index.html</welcome-file>
  <welcome-file>index.htm</welcome-file>
  <welcome-file>index.jsp</welcome-file>
  <welcome-file>default.html</welcome-file>
  <welcome-file>default.htm</welcome-file>
  <welcome-file>default.jsp</welcome-file>
 </welcome-file-list>
</web-app>
